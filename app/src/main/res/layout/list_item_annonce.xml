<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toTopOf="@+id/list_item_annonce_tv_Location"
        app:layout_constraintEnd_toStartOf="@+id/list_item_annonce_tv_Title"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@mipmap/ic_launcher_round" />

    <TextView
        android:id="@+id/list_item_annonce_tv_Title"
        android:layout_width="129dp"
        android:layout_height="22dp"
        android:text="Title"
        app:layout_constraintEnd_toStartOf="@+id/list_item_annonce_tv_NbPlace"
        tools:layout_editor_absoluteY="0dp" />

    <TextView
        android:id="@+id/list_tem_annonce_tv_Duree"
        android:layout_width="105dp"
        android:layout_height="22dp"
        android:text="DurÃ©e"
        tools:layout_editor_absoluteX="278dp"
        tools:layout_editor_absoluteY="0dp" />

    <TextView
        android:id="@+id/list_item_annonce_tv_NbPlace"
        android:layout_width="99dp"
        android:layout_height="22dp"
        android:text="Nb Place"
        app:layout_constraintEnd_toStartOf="@+id/list_tem_annonce_tv_Duree"
        tools:layout_editor_absoluteY="0dp" />

    <TextView
        android:id="@+id/list_item_annonce_tv_Location"
        android:layout_width="327dp"
        android:layout_height="19dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="Location"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/list_item_annonce_tv_Title" />

    <TextView
        android:id="@+id/list_item_annonce_tv_Description"
        android:layout_width="347dp"
        android:layout_height="47dp"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="4dp"
        android:lines="5"
        android:text="Description"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/list_item_annonce_tv_Location" />

    <TextView
        android:id="@+id/list_item_annonce_tv_Id"
        android:layout_width="33dp"
        android:layout_height="47dp"
        android:text="Id"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="100dp" />

</android.support.constraint.ConstraintLayout>